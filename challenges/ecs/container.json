[ {
    "name": "${container_name}",
    "image": "${ecr_repository}:${image_label}",
    "essential": true,
    "dnsSearchDomains": [],
    "logConfiguration": {
        "logDriver": "awslogs",
        "secretOptions": null,
        "options": {
            "awslogs-group": "osuctf-tf",
            "awslogs-region": "${aws_region}",
            "awslogs-stream-prefix": "${container_name}"
        }
    },
    "entryPoint": [],
    "portMappings": [${portMappingsJSON}],
    "command": [],
    "cpu": 0,
    "privileged": ${privileged},
    "environment": [${environmentJSON}],
    "dockerLabels": ${dockerLabelsJSON},
    "healthCheck": {
        "command": [ "CMD-SHELL", "(${healthcheck_cmd}) || exit 1" ],
        "retries": 2,
        "startPeriod": 30,
        "interval": 30,
        "timeout": 5
    }
  }
]